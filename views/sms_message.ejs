<% include partials/header%>
<% include partials/navbar%>
  <div class="container">
    <div class="page-header">
      <h1><%= data.body %></h1>
    </div>
    <div class="container col-lg-6 col-md-6">
      <div class="panel panel-default">
        <table class="table table-sm">
          <tr>
            <td>Status</td>
            <td><%= data.status %></td>
          </tr>
          <tr>
            <td>Created At</td>
            <td><%= data.created_at %></td>
          </tr>
          <tr>
            <td>Completed At</td>
            <td><%= data.completed_at %></td>
          </tr>
        </table>
      </div>

      <div class="panel panel-default">
        <div class="panel-heading">
          Recipient Counts
        </div>
        <table class="table table-sm">
          <% Object.keys(data.recipient_counts).map(function(k) { %>
            <tr>
              <td><%= k %></td>
              <td><%= data.recipient_counts[k] %></td>
            </tr>
          <% }) %>
        </table>
      </div>

    </div>
  </div>

<% include partials/footer%>
