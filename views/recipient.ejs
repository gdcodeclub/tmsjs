<% include partials/header%>
<% include partials/navbar%>
  <div class="container">
    <div class="page-header">
      <h1>Recipient detail for <%= data['email'] %></h1>
    </div>

    <div class="container col-lg-6 col-md-6">
      <div class="panel panel-default">
        <table class="table table-sm">
          <tr>
            <td>Status</td>
            <td><%= data.status %></td>
          </tr>
          <tr>
            <td>Created At</td>
            <td><%= data.created_at %></td>
          </tr>
          <tr>
            <td>Completed At</td>
            <td><%= data.completed_at %></td>
          </tr>
        </table>
      </div>

      <div class="panel panel-default">
        <div class="panel-heading">
          Open Events
        </div>
        <table class="table table-sm">
          <% opens.map(function(o){ %>
            <tr>
              <td><%= o.event_at %></td>
            </tr>
          <% }) %>
        </table>
      </div>

      <div class="panel panel-default">
        <div class="panel-heading">
          Click Events
        </div>
        <table class="table table-sm">
          <% clicks.map(function(o){ %>
            <tr>
              <td><%= o.event_at %></td>
            </tr>
          <% }) %>
        </table>
      </div>

    </div>

  </div>
<% include partials/footer%>
